{
  "css.validate": false,
  "editor.codeActionsOnSave": [
    "source.addMissingImports",
    "source.organizeImports",
    "source.fixAll.stylelint",
    "source.fixAll.eslint"
  ],
  "editor.formatOnSave": true,
  "eslint.options": {
    "extensions": ["md", "mdx"]
  },
  "eslint.validate": ["md", "mdx"]
}
